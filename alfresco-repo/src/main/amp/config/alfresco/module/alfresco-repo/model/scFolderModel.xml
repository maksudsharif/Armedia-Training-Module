<?xml version="1.0" encoding="UTF-8"?>
<!-- Definition of new Model -->
<model name="sc:somecofoldermodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

<!-- Imports are required to allow references to definitions in other models -->
	<imports>
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>		
	</imports>
	
		<!-- Introduction of new namespaces defined by this model -->
	<namespaces>
		<namespace uri="http://www.someco.com/model/scfolder/1.0" prefix="scf" />
	</namespaces>

	<type name="scf:invoiceFolder">
		<title>Invoice Folder</title>
		<parent>cm:folder</parent>
		<properties>
			<property name="scf:total">
				<type>d:double</type>
				<mandatory>true</mandatory>
			</property>
		</properties>
	</type>
	
	<type name="scf:invoiceType">
 		<title>Invoice Type</title>
 		<parent>cm:folder</parent>
 		<properties>
  			<property name="scf:invoiceDate">
   				<type>d:datetime</type>
   				<mandatory>true</mandatory>
  			</property>
  			<property name="scf:invoiceNumber">
   				<type>d:int</type>
   				<mandatory>true</mandatory>
  			</property>
  			<property name="scf:invoicevalue">
   				<type>d:double</type>
   				<mandatory>true</mandatory>
  			</property>
 		</properties>
	</type>












</model>